package com.mama.server.main.dao.model.crowd;

import java.util.Date;

/**
 * 众筹项目详情
 * @author majiafei
 *
 */
public class CrowdProjectPo
{
	//ID，主键
	private Integer id;
	
	//项目名称
	private String projectName;
	
	//列表中显示图片
	private Integer introdutionImageId;
	
	//视频url
	private String videoUrl;
	
	//简介
	private String briefIntroduction;
	
	//募集金额
	private double targetMoney;
	
	//开始时间
	private Date beginTime;
	
	//结束时间
	private Date endTime;
	
	//已募集金额
	private double raisedAmount;
	
	//项目故事
	private String projectStroy;
	
	//筹资方案
	private String projectPlan;
	
	//众筹回报
	private String reward;
	
	//状态
	private Integer status;
	
	//是否删除
	private Integer isRemoved;
	
	//创建时间
	private Date createTime;
	
	//修改时间
	private Date uppdateTime;

	public Integer getId()
	{
		return id;
	}

	public void setId(Integer id)
	{
		this.id = id;
	}

	public String getProjectName()
	{
		return projectName;
	}

	public void setProjectName(String projectName)
	{
		this.projectName = projectName;
	}

	public int getIntrodutionImageId()
	{
		return introdutionImageId;
	}

	public void setIntrodutionImageId(Integer introdutionImageId)
	{
		this.introdutionImageId = introdutionImageId;
	}

	public String getVideoUrl()
	{
		return videoUrl;
	}

	public void setVideoUrl(String videoUrl)
	{
		this.videoUrl = videoUrl;
	}

	public String getBriefIntroduction()
	{
		return briefIntroduction;
	}

	public void setBriefIntroduction(String briefIntroduction)
	{
		this.briefIntroduction = briefIntroduction;
	}

	public double getTargetMoney()
	{
		return targetMoney;
	}

	public void setTargetMoney(double targetMoney)
	{
		this.targetMoney = targetMoney;
	}

	public Date getBeginTime()
	{
		return beginTime;
	}

	public void setBeginTime(Date beginTime)
	{
		this.beginTime = beginTime;
	}

	public Date getEndTime()
	{
		return endTime;
	}

	public void setEndTime(Date endTime)
	{
		this.endTime = endTime;
	}

	public double getRaisedAmount()
	{
		return raisedAmount;
	}

	public void setRaisedAmount(double raisedAmount)
	{
		this.raisedAmount = raisedAmount;
	}

	public String getProjectStroy()
	{
		return projectStroy;
	}

	public void setProjectStroy(String projectStroy)
	{
		this.projectStroy = projectStroy;
	}

	public String getProjectPlan()
	{
		return projectPlan;
	}

	public void setProjectPlan(String projectPlan)
	{
		this.projectPlan = projectPlan;
	}

	public String getReward()
	{
		return reward;
	}

	public void setReward(String reward)
	{
		this.reward = reward;
	}

	public Integer getStatus()
	{
		return status;
	}

	public void setStatus(Integer status)
	{
		this.status = status;
	}

	public Integer getIsRemoved()
	{
		return isRemoved;
	}

	public void setIsRemoved(Integer isRemoved)
	{
		this.isRemoved = isRemoved;
	}

	public Date getCreateTime()
	{
		return createTime;
	}

	public void setCreateTime(Date createTime)
	{
		this.createTime = createTime;
	}

	public Date getUppdateTime()
	{
		return uppdateTime;
	}

	public void setUppdateTime(Date uppdateTime)
	{
		this.uppdateTime = uppdateTime;
	}
	
}
