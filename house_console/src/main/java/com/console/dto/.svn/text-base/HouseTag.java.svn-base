package com.console.dto;

/**
 * 房源标签
 * @author dengbiao
 *
 */
public class HouseTag {

	private Integer id;
	private String name;
	private Integer removed = 0;
	private Integer isDispayOnSearch = 0;
	
	public Integer getId() {
		return id;
	}
	public void setId(Integer id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public Integer getRemoved() {
		return removed;
	}
	public void setRemoved(Integer removed) {
		this.removed = removed;
	}
	public Integer getIsDispayOnSearch() {
		return isDispayOnSearch;
	}
	public void setIsDispayOnSearch(Integer isDispayOnSearch) {
		this.isDispayOnSearch = isDispayOnSearch;
	}
	
}