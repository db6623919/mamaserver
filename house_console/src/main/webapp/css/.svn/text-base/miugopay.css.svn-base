/*操作流程*/

.miugopay_process{ width:650px; height:50px; overflow:hidden; zoom:1; clear:both; margin:35px 0px 0px 68px; padding-left:20px; padding-bottom:25px; border-bottom:1px dashed #ccc;}
.miugopay_process ul li{ float:left; width:125px; height:auto; text-align:center;  }
.miugopay_process span{display:inline-block; vertical-align:middle; color:#fff; font-size:15px; width:26px; height:26px; line-height:26px; }
.miugopay_process p{ font-size:13px; margin:4px 0px 0px 20px;}
.miugopay_process em{display:inline-block; vertical-align:middle;  width:85px;}


.miugopay_process .process_complete span{  background:url(../images/process_bg.png) no-repeat left -26px; }
.miugopay_process .process_complete em{  border-bottom:2px solid #faa61a; }
.miugopay_process .process_complete p{ color:#9d9fa2; }

.miugopay_process .process_current span{ background:url(../images/process_bg.png) no-repeat left top;}
.miugopay_process .process_current em{ border-bottom:2px solid #f6810a; }
.miugopay_process .process_current p{ color:#58595b; }

.miugopay_process .process_current_nocompleted span{ background:url(../images/process_bg.png) no-repeat left -52px; }
.miugopay_process .process_current_nocompleted em{ border-bottom:2px solid #a7a9ac; }
.miugopay_process .process_current_nocompleted p{ color:#9d9fa2; }

.miugopay_process .process_current_nocompleted_success span{ background:url(../images/process_bg.png) no-repeat left -108px;  }
.miugopay_process .process_current_nocompleted_success em{ border-bottom:2px solid #f6810a; }
.miugopay_process .process_current_nocompleted_success p{ color:#58595b; }

.miugopay_process .process_current_nocompleted_nosuccess span{ background:url(../images/process_bg.png) no-repeat left -78px; }
.miugopay_process .process_current_nocompleted_nosuccess em{ border-bottom:2px solid #a7a9ac; }
.miugopay_process .process_current_nocompleted_nosuccess p{ color:#9d9fa2; }
