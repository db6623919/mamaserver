.top_bg .top ul .sitenav b{ font-size:12px; font-weight:normal; float:left; position:relative; padding:0 16px 0 11px; border:solid 1px #f7f7f7; border-bottom:none; margin-left:-11px;}
.top_bg .top ul .sitenav b s{ position:absolute; right:4px; top:10px; width:0; height:0; line-height:0; font-size:0; border:4px solid #f7f7f7; border-top-color:#999; overflow:hidden;}
.top_bg .top ul .sitenav .list{ clear:both; position:absolute; top:26px; left:-106px; background:#fff; border:solid 1px #ccc; width:150px; padding:0 10px; z-index:999; display:none;}
