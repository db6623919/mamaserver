.top_bg .top ul .sitenav .list .item strong{ display:block; font-weight:bold; color:#E6A11B; font-size:12px; text-align:right;}
.top_bg .top ul .sitenavhover .list{ display:block; clear:both;}
.top_bg .top ul .sitenavhover b{font-size:12px; border:1px solid #ccc; border-bottom:none; background:#fff; z-index:10000; margin-left:-12px; padding-left:12px;} 
.top_bg .top ul .sitenavhover b s{ position:absolute; right:4px; _right:5px; top:10px; width:0; height:0; line-height:0; font-size:0; border:4px solid #fff; border-top-color:#999; overflow:hidden;}


