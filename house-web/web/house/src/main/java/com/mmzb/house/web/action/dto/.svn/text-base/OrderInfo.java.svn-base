package com.mmzb.house.web.action.dto;

public class OrderInfo {
	private String orderId;
	private String houseId;
	/** 订单状态：9、审核通过；10、待审核；11、待入住； 15、审核通过；  */
	private String status;
	private int freezeAmt;
	private String totalAmt;
	private String payAmt;
	private String beginTime;
	private String endTime;
	private String linkmanName;
	private String linkmanPhone;
	private int memTotalAmt;
	private int orderAmt;
	private int prepayment;
	private String houseName;
	private int ammount;// 房间数
	private String buildingName; // 楼盘名称
	private String phone;// 咨询电话
	private int index;// index
	private String operTime; // 操作时间
	private int hotelCouponCount; // 使用旅居券数量
	private int hotelCouponDiscountAmt;// 旅居券抵扣金额
	private String verifyCode;
	private String pay_type;//支付方式
	
	
	public String getVerifyCode() {
		return verifyCode;
	}

	public void setVerifyCode(String verifyCode) {
		this.verifyCode = verifyCode;
	}

	public String getPayAmt() {
		return payAmt;
	}

	public void setPayAmt(String payAmt) {
		this.payAmt = payAmt;
	}

	/**
	 * 获取订单状态文本
	 * 
	 * @param status
	 * @return
	 */
	public static String getStatusText(int status) {
		switch (status) {
		case 9:
			return "待支付";
		case 10:
			return "待审核";
		case 11:
			return "待入住";
		case 12:
			return "已入住";
		case 13:
			return "已完成";
		case 15:
			return "待确认";
		case 17:
			return "已评价";
		case 23:
			return "已取消";
		case 24:
			return "预订失败";
		default:
			return "未知状态[" + status + "]";
		}
	}

	public String getOrderId() {
		return orderId;
	}

	public void setOrderId(String orderId) {
		this.orderId = orderId;
	}

