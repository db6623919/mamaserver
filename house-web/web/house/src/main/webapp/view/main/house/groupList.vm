			$(function() {
				var lazyload = {};
				lazyload.img = function(obj) {
					var img = typeof(obj) == 'object' ? obj : $(obj);
					var xz_src = img.attr('xz_src');
					if(xz_src && xz_src != 'ok') {
						img.attr('src', xz_src);
						img.attr('xz_src', 'ok');
					}
				}
				var lazy_load_imgs = $('.lazy_load_img');
				setTimeout(function() {
					lazy_load_imgs.mmlazyload({
						replaceImage: false,
						pxOffset: 50,
						memorySaver: true
					});
				}, 300);
