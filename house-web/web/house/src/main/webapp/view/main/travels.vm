	<script type="text/javascript">
		$(function() {
			//app 内部去掉标题
		  	if (navigator.userAgent.indexOf("mmsfang") != -1) {
			  	$('header').css('height','0px');
			}
		
			var lazyload = {};
			lazyload.img = function(obj) {
				var img = typeof(obj) == 'object' ? obj : $(obj);
				var xz_src = img.attr('xzsrc');
				if(xz_src && xz_src != 'ok') {
					img.attr('src', xz_src);
					img.attr('xzsrc', 'ok');
					if($('.clone')) {
						$('.clone').each(function() {
							$(this).attr('src', xz_src);
						})
					}
				}
			}
			var lazy_load_imgs = $('.lazy_load_img');
			setTimeout(function() {
				lazy_load_imgs.mmlazyload({
					replaceImage: false,
					pxOffset: 50,
					memorySaver: true
				});
			}, 300);
		})
	</script>

