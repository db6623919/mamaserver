    width: 100%;
    z-index:100;
}
.calendar-btn-ul{
  margin: 0 auto;
  padding:0;
}
.calendar-btn-ul li{
  float: left;
  width: 33.3%;
}
.leave .btn{
  padding:0;
  margin: 0;
  width: 100%;
  box-shadow: none;
  border:none;
}
.leave .btn ul>li b {
    display: block;
    height: 25px;
    line-height: 25px;
    text-align: center;
}
.leave .btn ul>li b font{
  color:#333333;
}
.room b {
    color: #646975;
    display: block;
    float: none;
    font-size: 16px;
    margin: 5px 20px 0 10px;
}
.sched b {
    color: #f75c52;    
    font-weight: normal;
}

.leave .show-w {
  margin-top:5px;
  height: 25px;
  line-height: 25px;
  text-align: center;
  font-size: 16px;
}
.leave .show-w big{
  color: #f75c52;
  font-size: 20px;
  margin: 0 5px;
}
