.reg_login{
  width: 65px;
    height: 48px;
    display: inline-block;
    color: #ffffff;
    text-align: center;
    line-height: 48px;
    font-size: 18px;
}

.reg_login:link {color: #ffffff;}               /* 未被访问的链接 */
.reg_login:visited {color: #ffffff;}            /* 已被访问过的链接 */
.reg_login:hover {color: #ffffff;}            /* 鼠标悬浮在上的链接 */
.reg_login:active {color: #ffffff;}   /* 鼠标点中激活链接 */ 

.title {
    text-align: center;
    font-size: 18px;
    line-height: 48px;
    color: #fff;
}
.more_wraper {position: absolute;/*width: 12%;*/ margin-left: 1%; height: 48px;top: 0px;right: 0; font-size: 12px;}
.table-condensed tr th{
  border-bottom: 1px solid #e7e7e7;
}
.table-condensed tr th.month{
  border-bottom: none;
  padding: 10px 0;
}
.table-condensed tr td{border-right:1px solid #fff;line-height:45px; font-size:14px; width:14.285%;color:#808080;}
.table-condensed tr th{font-weight:normal; font-size:16px; color:#808080;}
.daterangepicker {
  background-color: #fbf7ec;
}
.table-condensed tr{
  border-bottom: 1px solid #e7e7e7;
}
.daterangepicker b{
  font-weight: normal;
}
.daterangepicker th.month{color:#f75c52;font-size: 20px;}
.available i{ color:#f75c52; font-style:normal;}
.clear{ clear:both;}
 .daterangepicker.dropdown-menu {
  max-width: none;
  z-index: 99;
  margin:0 -1px;
}
.daterangepicker.opensleft .ranges, .daterangepicker.opensleft .calendar {
  float: left;
  margin: 4px;
}

/*.daterangepicker.opensright .ranges, .daterangepicker.opensright .calendar {*/
.daterangepicker.opensright .calendar {
   float:left; 
   margin:5px 2.5%;
   width: 95%;
   border:1px solid #cececd;
}

.daterangepicker .ranges {
  margin:115px 0px 0px 0px;
  text-align: left;
  padding:0px 20px;
}
.daterangepicker .noBanner { margin:5px 0px 0px 0px;}

.daterangepicker .ranges .range_inputs>div {
  float: left;
}

.daterangepicker .ranges .range_inputs>div:nth-child(2) {
  padding-left: 11px;
}

.daterangepicker .ranges .range_inputs .applyBtn{
	float:right; 
}
.daterangepicker .ranges .range_inputs .cancelBtn{
	float:right;margin-right:10px;
}


.daterangepicker .calendar {
  display: none;
  width:100%; max-width:inherit;
}

.daterangepicker.show-calendar .calendar {
    display: block;
}

.daterangepicker .calendar.single .calendar-date {
  border: none;
}

.daterangepicker .calendar th, .daterangepicker .calendar td {
  font-family: 'Helvetica Neue', Helvetica, Arial, sans-serif;
  white-space: nowrap;
  text-align: center;
  min-width: 32px;
  min-height: 51px;
  padding: 2px 0;
}
.daterangepicker .calendar th b, .daterangepicker .calendar td b{
  padding-top:10px;
}
.daterangepicker .daterangepicker_start_input label,
.daterangepicker .daterangepicker_end_input label {
  color: #333;
  display: block;
  font-size: 14px;
  font-weight: normal;
  height: 20px;
  line-height: 20px;
  margin-bottom: 2px;
  text-shadow: #fff 1px 1px 0px;
  text-transform: uppercase;
  padding-right:10px;
  float:left;
  height:34px;
  line-height:34px;
}

.daterangepicker .ranges input {
  font-size: 11px;
}

.daterangepicker .ranges .input-mini {
	border:none;
	background:#FFF;
  color: #555;
  display: block;
  font-size: 14px;
  height: 34px;
  line-height: 34px;
  vertical-align: middle;
  width: 84px;
  float:left;
}

.daterangepicker .ranges ul {
  list-style: none;
  margin: 0;
  padding: 0;
}

.daterangepicker .ranges li {
  font-size: 13px;
  background: #f5f5f5;
  border: 1px solid #f5f5f5;
  color: #08c;
  padding: 3px 12px;
  margin-bottom: 8px;
  -webkit-border-radius: 5px;
  -moz-border-radius: 5px;
  border-radius: 5px;
  cursor: pointer;
}

.daterangepicker .ranges li.active, .daterangepicker .ranges li:hover {
  background: #08c;
  border: 1px solid #08c;
  color: #fff;
}

.daterangepicker .calendar-date {
	margin-right: 1px;
  background: #fff;}

.daterangepicker .calendar-time {
  text-align: center;
  margin: 8px auto 0 auto;
  line-height: 30px;
}


.daterangepicker.opensleft:before {
  position: absolute;
  top: -7px;
  right: 9px;
  display: inline-block;
  border-right: 7px solid transparent;
  border-bottom: 7px solid #ccc;
  border-left: 7px solid transparent;
  border-bottom-color: rgba(0, 0, 0, 0.2);
  content: '';
}

.daterangepicker.opensleft:after {
  position: absolute;
  top: -6px;
  right: 10px;
  display: inline-block;
  border-right: 6px solid transparent;
  border-bottom: 6px solid #fff;
  border-left: 6px solid transparent;
  content: '';
}


.daterangepicker table {
  color:#000;
  width: 100%;
  margin: 0;
  border-collapse:collapse;
}

.daterangepicker td, .daterangepicker th {
  text-align: center;
  width: 20px;
  height: 51px;
  cursor: pointer;
  white-space: nowrap;
}

.daterangepicker td.off {
  color:#CCC;
}

.daterangepicker td.disabled {
  color: #999;
}

.daterangepicker td.available:hover, .daterangepicker th.available:hover {
  background: #eee;
}

.daterangepicker td.in-range {
  background: #f75c52;
  color:#FFF;
  -webkit-border-radius: 0;
  -moz-border-radius: 0;
  border-radius: 0;
}

.daterangepicker td.active, .daterangepicker td.active:hover {
  color: #fff;
  background:#f75c52;

}

.daterangepicker td.start-date,.daterangepicker td.end-date, .daterangepicker td.start-date:hover, .daterangepicker td.end-date:hover {
    background: none repeat scroll 0 0 #f75c52;
    color: #FFFFFF;
}
.daterangepicker td.no-room-tr,.daterangepicker td.no-room-tr:hover{
    background: none repeat scroll 0 0 #d7d7d7;
    color:#ffffff;
}
.daterangepicker td.week, .daterangepicker th.week {
  font-size: 80%;
  color: #ccc;
}

.daterangepicker select.monthselect, .daterangepicker select.yearselect {
  font-size: 12px;
  padding: 1px;
  height: auto;
  margin: 0;
  cursor: default;
}

.daterangepicker select.monthselect {
  margin-right: 2%;
  width: 56%;
}

.daterangepicker select.yearselect {
  width: 40%;
}

.daterangepicker select.hourselect, .daterangepicker select.minuteselect, .daterangepicker select.ampmselect {
  width: 50px;
  margin-bottom: 0;
}

.daterangepicker_start_input {
  float: left;
  margin-right:10px;
}

.daterangepicker_end_input {
  float: left; 
  padding-left: 11px
}

.daterangepicker th.month {
  width: auto;
}
.perice-off,.no-room,.perice-down{display:block; font-size:12px; color:#999;}
.perice-down{color:#fff;}
.no-room{ color:#FFF;}

/*日期控件提示tips*/
.tc-ar{ position:fixed; top:50%; z-index:100000000000; width:100%; text-align:center;}
.tc-ar b{ padding:10px 20px; background:#000; border-radius:3px;opacity:0.8;color:#fff}

/*顶部浮动*/
.am-fixed {
    left: 0;
    position: fixed;
    top: 0;
    width: 100%;
    z-index: 9999;
}