/* 我的任我行首页 */
body{
	overflow-x:hidden 
}
.card-info img{
	width: 100%;
}
.am-ucenter-next{
    float: right;
    height: 1.2rem;
    width: 1.2rem;
}
.am-ucenter-title{
	height: 4.4rem;
	line-height: 4.4rem;
    font-size: 1.4rem;
    color:#757575;
}
.reg_login {
    height: 44px;
    display: inline-block;
    color: #ffffff;
    text-align: center;
    line-height: 44px;
    font-size: 1.3rem;
    margin-top: 4px;
    padding-right: .5rem;
}
.reg_button{
    background-color: #f75c52;
    color: #ffffff;
    margin-left: 4.5rem;
    margin-right: 4.5rem;
    border-radius: 5px;
    line-height: 4rem;
    text-align: center;
    font-size: 1.5rem;
}
.a_hand{
        cursor: pointer;
    }
/*可共享有情感*/
.bg-share{
    position: relative;

}
.bg-share-content{
    position: absolute;
    top: 1.5rem;
    left:1.5rem;
}
.bg-share img{
    width:100%;
    height: 100%;
}
.am-title-border
{
    display: inline-block;
    width: 0.3rem;
    height: 1.2rem;
    line-height: 1.2rem;
    background:url(../i/common/title_border.png) no-repeat;
    background-size: 100%;
}
/*可买房有溢价*/
.vip-buy p{
    margin: 0;
    color: #757575;
}
/* 可消费与低价*/
table{
      border-collapse:inherit;
}
table td,table th{
    text-align: center;
    font-size: 1.1rem;
    font-weight: normal;
    line-height: 2.5rem;
    color:#757575;
}
table th{
    color: #fff;
    background: #f75c52;
    border-bottom: 1px solid #FFF;
    border-right: 1px solid #fff;
}
table tr.white{
    background: #fff;
}
table tr.grey{
    background: #f2f2f2;
}
table td{
    border-right: 1px solid #c7c7c7;
    border-left: 1px solid #c7c7c7;
}
table td.border-left-none{
    border-left:none;
}
table th.border-right{
    border-right:1px solid #f75c52;
}
table td.bor-bottom{
    border-bottom: 1px solid #c7c7c7;
}