/*list页*/
.top-fixed
{
	position:fixed;
	top: 0;
	z-index: 100;
	width: 100%;
}

.title-logo
{
	display:block;
	width:25%;
	margin:0 auto; 
	height: 48px;
	line-height: 43px;
}

.location-head, .location-head > *
{
	height: 40px;
	line-height: 40px !important;
}

.location-title
{
	text-align: center;
    font-size: 1.3rem;
    line-height: 48px;
}

.user-icon
{
	background-image: url(../i/common/user.png);
	position: absolute;
	right: 0;
	top: 0;
}

.category-wrapper
{
	padding: 0 1rem;
}

.category-block
{
	height: 60px;
	padding: 0;
	/*margin-left: 1rem;*/
	/*line-height: 60px;*/
}

.category-block img
{
	width: 1.8rem;
	height: 1.8rem;
	margin: 7px 0 3px;
}

.category-block p
{
	margin: 0;
	line-height: normal;
}

.category-block .active
{
	color: #f74f5d;
}

.category-block.active
{
	border-bottom: 2px solid #f74f5d;
}

.info-list
{
	margin: .5rem 0;
	padding: 0 0 0 1rem;
	color: #fff;
}

.info-list > span
{
	display: inline-block;
	line-height: 1.3rem;
	text-indent: 0;
	padding: .3rem .5rem;
	border-top-left-radius: 15px;
	border-bottom-left-radius: 15px;
	border-top-right-radius: 15px;
	border-bottom-right-radius: 15px;
}

.info-list .room
{
	background-color: #7da8ca;
}

.info-list .people
{
	background-color: #72c3bc;
}

.info-list .set
{
	background-color: #96bf23;
}

.info-list .total
{
	background-color: #ed6a51;
}

.info-list .expected
{
	background-color: #f86c63;
}

.info-list .free
{
	background-color: #f89b63;
}

.info-list .limitation
{
	background-color: #f8c963;
}

.new-collection
{
	background-image: url(../i/common/new_before_collect.png);
}

.new-collection-active
{
	background-image: url(../i/common/new_after_collect.png);
}

.hidden
{
	display: none;
}

.btn
{
	border-radius: 2px;
	height: 3rem;
	line-height: 3rem;
	font-size: 1.3rem;
	width: 9rem;
	margin: 0 auto;
	color: #fff;
}

.btn-danger
{
	background-color: #f75258;
}

.ad-tips
{
	color: #f75258;
	margin-top: .2rem;
}

.pos-relative
{
	position: relative;
}

.pos-absolute
{
	position: absolute;
}

.pos-right-10
{
	right: 1rem;
}

.text-delete
{
	text-decoration: line-through;
}

/*旅居宝*/
.collect-amount
{
	position: absolute;
	left: 1rem;
	top: .5rem;
	color: #fff;
}

.project-progress
{
	background-color: rgba(0,0,0,.6);
	width: 5rem;
	height: 6.5rem;
	position: absolute;
	top: 0;
	right: 1rem;
	border-bottom-left-radius: 3rem;
	border-bottom-right-radius: 3rem;
}

.project-progress p
{
	margin-bottom: .3rem;
}

.project-progress .progress-text
{
	position: absolute;
	top: 1.5rem;
	left: 1rem;
	color:#fff;
	font-size: 1.1rem;
}

.progress-bar-cover
{
	width: 4.2rem;
	height: 4.2rem;
	border-radius: 50%;
	/*margin: .5rem auto;*/
	background-color: #c6c6c6;
	background: -webkit-radial-gradient(transparent 1.7rem, #f75c52 1.7rem);
	background: radial-gradient(transparent 1.7rem, #f75c52 1.7rem);
	background: -moz-radial-gradient(transparent 1.7rem, #f75c52 1.7rem);
	background: -ms-radial-gradient(transparent 1.7rem, #f75c52 1.7rem);
	background: -o-radial-gradient(transparent 1.7rem, #f75c52 1.7rem);
	position: absolute;
	left: .4rem;
}

.progress-bar-outer-left, .progress-bar-outer-right
{
	width: 4.2rem;
	height: 4.2rem;
	position: absolute;
	top: 0;
	left: 0;
}

.progress-bar-outer-left, .progress-bar-inner-left
{
	clip: rect(0, 2.1rem, auto, 0);
}

.progress-bar-outer-right, .progress-bar-inner-right
{
	clip: rect(0, auto, auto, 2.1rem);
}

.progress-bar-inner-left, .progress-bar-inner-right
{
	width: 4.2rem;
	height: 4.2rem;
	border-radius: 50%;
	background-color: #c6c6c6;
	background: -webkit-radial-gradient(50% 50%, transparent 1.7rem, #c6c6c6 
		1.7rem);
	background: radial-gradient(50% 50%, transparent 1.7rem, #c6c6c6 1.7rem);
	background: -moz-radial-gradient(50% 50%, transparent 1.7rem, #c6c6c6 1.7rem);
	background: -ms-radial-gradient(50% 50%, transparent 1.7rem, #c6c6c6 1.7rem);
	background: -o-radial-gradient(50% 50%, transparent 1.7rem, #c6c6c6 1.7rem);
	position: absolute;
}

