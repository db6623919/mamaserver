/*表单*/
.emulate-select, .emulate-input
{
	height: 3rem;
	line-height: 3rem;
	border: 1px solid #cbcbcb;
	padding: 0 .5rem;
	font-size: 1.2rem;
	margin: 0 1.2rem 1.2rem 6rem;
	color: #757575;
	position: relative;
}

input[type=number]::-webkit-inner-spin-button
{    
	-webkit-appearance: none;
}

.emulate-input input
{
	height: 100%;
    border: none;
    position: absolute;
}

.emulate-list
{
	border: 1px solid #cbcbcb;
	position: absolute;
	top: 2.9rem;
	left: 0;
	z-index: 1;
	padding-left: .5rem;
	max-height: 10rem;
	overflow: scroll;
	background-color: #fbf7ec;
}

.emulate-list p
{
	line-height: 3rem;
	margin: 0;
}

.emulate-list p:not(:last-child)
{
	border-bottom: 1px solid #cbcbcb;
}

.emulate-count
{
	width: 12.8rem;
	height: 3rem;
	line-height: 3rem;
	border: 1px solid #cbcbcb;
	margin: 0 1.2rem 1.2rem 6rem;
}

.emulate-count > .emulate-minus, .emulate-count > .emulate-plus
{
	width: 3.2rem;
	font-size: 1.5rem;
	float: left;
	text-align: center;
	color: #cbcbcb;
}

.emulate-count > .emulate-number
{
	width: 6.2rem;
	text-align: center;
	font-size: 1.2rem;
	float: left;
	border-left: 1px solid #cbcbcb;
	border-right: 1px solid #cbcbcb;
	color: #757575;
}

.form-block .form-content-group:first-child
{
	margin-top: 1.2rem;
}

.form-label
{
	height: 3rem;
	line-height: 3rem;
}

.form-select-more
{
	position: absolute;
	top: 1rem;
	right: 1rem;
	width: 1rem;
	transform: rotate(90deg);
	transform: -webkit-rotate(90deg);
	transform: -moz-rotate(90deg);
	transform: -ms-rotate(90deg);
	transform: -o-rotate(90deg);
}

.form-calendar
{
	width: 1.5rem;
	top: .75rem;
	transform: initial;
}

.form-content-text
{
	font-size: 1.2rem;
	height: 3rem;
	line-height: 3rem;
	color: #757575;
	margin-left: 6rem;
}

.btn-group.form-btn
{
	margin-bottom: 1rem;
}

.btn-group.form-btn > button
{
	width: 100%;
}

.form-tips
{
	position: absolute;
	top: .7rem;
	right: 2.5rem;
}

