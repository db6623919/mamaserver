/*筛选条件*/

.search-condition {
	height: 45px;
	background-color:#fff;
	color: #474747;
	font-size: 1.1rem;
	border-bottom: solid 1px #D3D3D3;
}

.search-condition li {
	position: relative;
	float: left;
	width: 25%;
	height: 45px;
	line-height: 45px;
	text-align: center;
	white-space: nowrap;
	overflow: hidden;
	text-overflow: ellipsis;
}

.m-active {
	color: #FF5858;
}

.search-condition li span{
	position: relative;
    display: inline-block;
    max-width: 100%;
    padding-right: 10px;
    vertical-align: top;
}

.search-condition li:not(:first-child):before {
    content: "";
    width: 0;
    height: 14px;
    position: absolute;
    left: 0;
    top: 50%;
    margin-top: -7px;
    border-left: 1px solid #DBDBDB;
    -webkit-transform: scaleX(.5);
    transform: scaleX(.5);
}

.search-condition li span:after {
	content: "";
	position: absolute;
	top: 50%;
	margin-top: -3px;
	right: 1px;
	height: 0;
	width: 0;
	border-style: solid;
	border-width: 4px 4px 0 4px;
	border-color: transparent;
	border-top-color: #ccc;
}

.search-condition li.m-active span:after {
    transform-origin: center center;
    -webkit-transform: rotate(180deg);
    transform: rotate(180deg);
	border-top-color: #ff5858;
}
.sel-part {
	display: none;
	position: absolute;
	width: 100%;
	font-size: 1.1rem;
	font-weight: 400;
	color: #555;
}

.daterangepicker.dropdown-menu {
	margin: 0;
}
			
.daterangepicker .ranges {
	margin: 0;
}

/**
 *位置区域 
 */

.sel-position {
	display: none;
	z-index: 111;
	background-color: #fff
}

.sel-part ul li {
	height: 45px;
	line-height: 45px;
	padding-left: 1.5rem;
	border-bottom: solid 1px #D3D3D3;
}

.sel-position ul li:not(:first-child) {
	margin-left: 1.5rem;
	padding-left: 0;
}


/*更多筛选*/

.sel-more {
	padding-bottom:5rem;
	background-color: #efeff4;
	z-index: 112;
}

.more-container {
	background-color: #fff;
	margin-left: .8rem;
	margin-right: .8rem;
	margin-top: 1.5rem;
	padding-top: 1rem;
	-webkit-border-radius: 8px;
	-moz-border-radius: 8px;
	border-radius: 8px;
}

.more-container> p {
	color: #000;
	text-align: center;
}

.price-container {
	text-align: center;
	padding-bottom: 2rem;
	margin-top: 2rem;
}

.price-container span {
	padding: .5rem .5rem;
	border: solid 1px #E8E8E8;
	-webkit-border-radius: 4px;
	-moz-border-radius: 4px;
	border-radius: 4px;
}

.price-container span.m-active{
	color: #fff;
	background-color: #FF5858;
	border-color: #FF5858;
}


.price-container span:first-child {
	margin-left: 0;
}

.price-container span.active {
	color: #fff;
	background-color: #ff5858;
	border-color: #FF5858;
}


/*入住人数&&居室*/
.more-container.count {
  padding-top: 0;
}
.count-container {
  position: relative;
  text-align: center;
  height: 4rem;
  line-height: 4rem;
}

.more-container .count-container:first-child {
  border-bottom: solid 1px #D3D3D3;
}

.count-container .minus {
  position: absolute;
  left: 1.5rem;
}

.count-container .minus img,
.count-container .plus img {
  vertical-align: middle;
  width: 2rem;
}

.count-container .plus {
  position: absolute;
  right: 1.5rem;
}
/*房间特色*/

.tag-container {
	padding: 0;
}

.more-container .tags {
	text-align: center;
	height: 3rem;
}

.more-container .tags div {
	position: relative;
	float: left;
	width: 33.33%;
	height: 3rem;
	line-height: 3rem;
	background-color: #fff;
}

.more-container .tags div.m-active {
	background: url(http://file.88mmmoney.com/5c01976e-94ca-4d7d-9528-d5fcd5aeac6e) no-repeat 100% 100%;
	background-size: 1.5rem auto;
}

.more-container .tags:not(:last-child) div {
	border-bottom: solid 1px #D3D3D3;
}

.more-container .tags div:not(:last-child) {
	border-right: solid 1px #D3D3D3;
}

.more-container .tags:first-child div:first-child {
	-webkit-border-top-left-radius: x;
	-moz-border-radius-topleft: 4px;
	border-top-left-radius: 4px;
}

.more-container .tags:first-child div:last-child {
	-webkit-border-top-right-radius: x;
	-moz-border-radius-topright: 4px;
	border-top-right-radius: 4px;
}

.more-container .tags:last-child div:first-child {
	-webkit-border-bottom-left-radius: 4px;
	-moz-border-radius-bottomleft: 4px;
	border-bottom-left-radius: 4px;
}

.more-container .tags:last-child div:last-child {
	-webkit-border-bottom-right-radius: 4px;
	-moz-border-radius-bottomright: 4px;
	border-bottom-right-radius: 4px;
}


