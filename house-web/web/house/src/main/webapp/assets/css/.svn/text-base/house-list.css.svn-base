}

.nodata-tips .clear-more {
  width: 30%;
  height: 3.5rem;
  line-height: 3.5rem;
  margin-top: 1.5rem;
  margin-left: 35%;
  background-color: #fff;
  color: #FF5858;
  border: solid 1px #FF5858;
  -webkit-border-radius: 6px;
  -moz-border-radius: 6px;
  border-radius: 6px;
}

