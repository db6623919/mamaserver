/*推荐排序*/

.sel-sort {
	display: none;
	z-index: 113;
	background-color: rgba(255, 255, 255, .7);
}

.sel-sort ul {
	background-color: #fff;
}

.sel-sort ul li {
	margin-left: 1.5rem;
	padding: 0;
}

.sel-part ul li:last-child {
	border-bottom: none;
}


/*关键字搜索*/

.key-search {
	padding-left: .7rem;
	margin-top: 1.5rem;
	color: #333;
}

.key-container {
	padding: 0;
}

.key-container input {
	border: none;
	width: 100%;
	height: 3rem;
	line-height: 3rem;
	padding-left: .7rem;
	padding-right: .7rem;
	background-color: transparent;
}


/*确定||取消*/

.more-bottom {
	position: fixed;
	bottom: 0;
	background-color: #fff;
	height: 4rem;
	line-height: 4rem;
	width: 100%;
}

.more-bottom> div {
	position: absolute;
	bottom: .5rem;
	width: 40%;
	height: 3rem;
	line-height: 3rem;
	text-align: center;
	border: solid 1px #D3D3D3;
	-webkit-border-radius: 8px;
	-moz-border-radius: 8px;
	border-radius: 8px;
}

#clear-more {
	left: 1.5rem;
}
#more-confirm {
	right: 1.5rem;
	color: #FF5858;
}

/**
 * 无数据提示
 */

.nodata-tips {
  position: absolute;
  width: 100%;
  height: 1000%;
  background-color: #EBEBEB;
  font-size: 1.1rem;
  text-align: center;
}

.nodata-tips>p {
  margin-top: 20%;
