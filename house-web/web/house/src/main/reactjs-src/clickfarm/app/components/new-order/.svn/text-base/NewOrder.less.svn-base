@import '../common/common.less';
@import '../common/variable';

.shopName {
	font-size: 1.2rem;
	text-align: center;
	padding: 20px 0;
	background-color: #fff;
}

.highlight {
	color: @highlight-color;
}
.input {
	width:80%;
	height: 100%;
	border:none;
	outline: none;
	font-size: 1rem;
}

.ok {
	background-color: @primary-color;
	color:#fff;
	height: 80%;
	line-height: 80%;
	padding: 0 0.8rem;
	// border-radius: 3px;
	margin-right: 6px;
}
.arrow,.weixin,.check {
	display: inline-block;
	background-repeat: no-repeat;
	background-size: contain;
	background-position: center;
	vertical-align: middle;
}
.weixin {
	width: 2rem;
	height: 2rem;
	margin-right: 1rem;
	background-image: url(data:image/png;base64,);
}

.check {
	margin-top: 0.6rem;
	width:2rem;
	height: 2rem;
	background-image: url(data:image/png;base64,)
}

.submit {
	width: 90%;
	height: 3rem;
	line-height: 3rem;
	background-color: @primary-color;
	color:#fff;
	margin:20px auto;
	display: block;
	font-size: 1.4rem;
	&[disabled] {
		background-color: #aaa;
	}
}

